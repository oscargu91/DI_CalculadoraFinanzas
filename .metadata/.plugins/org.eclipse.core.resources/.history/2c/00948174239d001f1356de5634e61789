package com.carballeira.controller;

import com.carballeira.view.FinanceEntry;
import com.carballeira.view.Main;

public class FinanceController {
	private FinanceEntry model;
	private Main vista;

	public FinanceController(FinanceEntry model, Main vista) {
		this.model = model;
		this.vista = vista;
	}

	public FinanceController(Main vista) {
		this.vista = vista;
	}

	
	
	
	public void comprobarDatos(String nombre, String cantidad, String tipo) {

		model.validarEntrada(nombre, cantidad, tipo);

	}

}
